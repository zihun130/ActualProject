        <!-- Required. For publish channel feature -->
        <!-- JPUSH_CHANNEL 是为了方便开发者统计APK分发渠道。 -->
        <!-- 例如: -->
        <!-- 发到 Google Play 的APK可以设置为 google-play; -->
        <!-- 发到其他市场的 APK 可以设置为 xxx-market。 -->
        <!-- 目前这个渠道统计功能的报表还未开放。 -->


        <!-- Required  . Enable it you can get statistics data with channel -->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" />
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="838174c9b4cb813b1f1edcb8" /> <!-- </>值来自开发者平台取得的AppKey -->

        <activity android:name=".login.LoginActivity" />
        <activity android:name=".activity.ShowViewActivity" />
        <activity android:name=".activity.CaptureActivity" />
        <activity android:name=".activity.CommentActivity" />
        <activity android:name=".login.RegisterActivity" />
        <activity android:name=".activity.IndentDetailsActivity" />

        <!-- alipay sdk begin -->
        <!-- 必须集成的不然就是支付失败 -->
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation|screenSize"
            android:exported="false"
            android:screenOrientation="behind"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name=".personal_center.IJKVideoActivity"></activity>
    </application>

</manifest>